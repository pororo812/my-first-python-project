5)

homework1 = "algebra"
homework2 = "physics"
homework3 = "kazakh language"

subjects = [homework1, homework2, homework3] #Покажет списки по данному порядку
print(subjects)



6)

production1 = ["sofa, carpet"]
production2 = ["cofee, candies"]
my_list = production1 + production2 #Покажет элементы по порядку

print(my_list)


7)
homework = ["algebra", "physics", "kazakh language"]
homework.append("English") #Добавляет к тек.списку элемент English

print(homework)


8)
title = "Welcome, Uliana!"
new_title = title.replace("Uliana", "students") #Заменяет слово/букву/цифру на другое

print(title)
print(new_title)


9) homework = ["algebra", "physics", "kazakh language"]

homeworks = homework.pop() #удаляет последний эл. можно указать индекс в скобках
print(homework)
print(homeworks)

10)
homework = ["algebra", "physics", "kazakh language"]
homework2 = ["English", "Geometry"]
homework.extend(homework2) #совмещает списки
print(homework)

11)
homework = ["algebra", "physics", "kazakh language"]
homework.reverse() #пишет список наоборот
print(homework

12)
my_list = [8, 2,90,546, 5, 11]
my_list.sort() #числа отсертерует по возрастанию
print(my_list)
